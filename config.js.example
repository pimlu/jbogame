module.exports = config;
var fs=require('fs');
var config = {};
config.proxy={};//head of everything
config.front={};//frontend website
config.game={};//gameservers
config.proxy.workers=1;
config.proxy.port=8080;
config.proxy.ports=8443;
config.front.workers=1;
config.front.port=1300;
config.front.https=function() {
  return {
    key: fs.readFileSync('./proxy/https/zdelu.key'),
    cert: fs.readFileSync('./proxy/https/zdelu.crt')
  };
};
config.front.rounds=8;
config.front.cd={};
config.front.cd.dir='game';
var appjs='./server/app.js';
config.game.types={
  'mix':appjs,
  'jbo':appjs,
  'eng':appjs,
  'commtest':'./debug/js/commserve.js'
};
config.game.worlds={};
config.game.worlds[1]=['mix',8081];
config.knex={
  client:'pg',
  connection: {
    host:'127.0.0.1',
    user:'postgres',
    database:'jbogame'
  }
};
config.redis={
  host:'127.0.0.1',
  port:6379,
  options:{}
};
config.brute=function() {
  // stores state locally, don't use this in production
  var ExpressBrute=require('express-brute'),
  store=new (ExpressBrute.MemoryStore)();
  return new ExpressBrute(store,{
    maxWait:600000//10m
  });
};
module.exports = config;
